{
  "name": "nutoff-wallet",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "bin": {
    "cashu": "./index.ts"
  },
  "scripts": {
    "format": "bun run prettier --write .",
    "setup": "bun install && bun run --bun scripts/setup.ts && bun run start",
    "start": "bun run --bun scripts/start.ts",
    "stop": "bun run --bun scripts/stop.ts",
    "startup": "bun install && bun run setup"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@cashu/cashu-ts": "^2.7.3",
    "@contextvm/sdk": "^0.1.37",
    "@modelcontextprotocol/sdk": "^1.20.2",
    "zod": "^3.25.76"
  }
}
